set title "CPU core temperature"
unset multiplot
set xdata time
set style data lines
set terminal png
set timefmt "%Y-%m-%d-%H:%M:%S"
set format x "%m-%d\n%H:%M"
set xlabel "Date/Time"
set ylabel "CPU Core temp Â°C"
set yrange [0:50]
set autoscale x
set output "coretemp.png"
plot 'log' using 1:2 t "CPU Core Temp." w lines, 'log' using 1:3 t "HDD Temp."
